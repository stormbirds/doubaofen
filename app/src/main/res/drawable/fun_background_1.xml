<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape>
            <solid android:color="@android:color/transparent"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
    <item android:left="1px" android:top="1px" android:right="1px" android:bottom="1px">
        <shape>
            <gradient android:startColor="#ff5c75" android:endColor="#fe8b86" android:angle="90" android:type="linear" android:centerX="0.5" android:centerY="0.5"/>
            <padding android:left="2dp" android:top="2dp" android:right="2dp" android:bottom="2dp"/>
            <corners android:radius="3dp"/>
        </shape>
    </item>
</layer-list>
