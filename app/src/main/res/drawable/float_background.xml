<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape>
            <solid android:color="@android:color/transparent"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
    <item android:left="1px" android:top="1px" android:right="1px" android:bottom="1px">
        <shape>
            <gradient android:startColor="@color/color_C5_1" android:endColor="@color/color_C5_1" android:angle="90" android:type="linear" android:centerX="0.5" android:centerY="0.5" android:centerColor="@color/color_C5_1"/>
            <padding android:left="4dp" android:top="4dp" android:right="4dp" android:bottom="4dp"/>
            <corners android:radius="4dp"/>
        </shape>
    </item>
</layer-list>
